{
  "name": "scaffolding",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run watch-ts\" \"npm run watch-js\" \"./scripts/db.sh\" \"npm run db\" \"./scripts/wait.sh\"",
    "build": "npm run build-ts",
    "watch-ts": "tsc --watch",
    "watch-js": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"npm run watch-ts\" \"npm run watch-js\"",
    "build-ts": "tsc",
    "db": "sequelize db:migrate && sequelize db:seed:all",
    "test": "jest --forceExit --coverage --detectOpenHandles --verbose false",
    "watch-test": "npm run test -- --watchAll",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run watch\" \"npm run client\""
  },
  "author": "Kirsi Trospe",
  "license": "MIT",
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lusca": "^1.6.2",
    "@types/node": "^14.14.31",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/winston": "^2.4.4",
    "aws-sdk": "^2.848.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "lusca": "^1.6.1",
    "nodemailer": "^6.6.2",
    "nodemailer-mailgun-transport": "^2.1.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "supertest": "^6.1.3",
    "typeorm": "^0.2.31",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^24.9.0",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.2",
    "typescript": "^4.1.5"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
